{
  "name": "@royalty-loans/admin",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/vite:build",
        "options": {
          "outputPath": "dist/apps/admin",
          "configFile": "apps/admin/vite.config.ts"
        }
      },
      "codegen": {
        "executor": "nx:run-commands",
        "options": {
          "command": "graphql-codegen",
          "cwd": "apps/admin"
        }
      },
      "generate": {
        "executor": "nx:run-commands",
        "options": {
          "command": "wagmi generate",
          "cwd": "apps/admin"
        }
      }
    }
  }
}
