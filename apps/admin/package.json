{
  "name": "@royalty-loans/admin",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/vite:build",
        "options": {
          "outputPath": "apps/admin/dist",
          "configFile": "apps/admin/vite.config.ts"
        }
      },
      "codegen": {
        "executor": "nx:run-commands",
        "options": {
          "command": "graphql-codegen",
          "cwd": "apps/admin"
        }
      },
      "generate": {
        "executor": "nx:run-commands",
        "options": {
          "command": "wagmi generate",
          "cwd": "apps/admin"
        }
      },
      "lint": {
        "executor": "nx:run-commands",
        "options": {
          "command": "eslint \"{src,test}/**/*.ts\" --fix",
          "cwd": "apps/admin"
        }
      },
      "typecheck": {
        "executor": "nx:run-commands",
        "options": {
          "command": "tsc --noEmit -p apps/admin/tsconfig.app.json"
        }
      }
    }
  }
}